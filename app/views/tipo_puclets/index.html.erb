<p id="notice"><%= notice %></p>

<h1>Tipo Puclets</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_puclets.each do |tipo_puclet| %>
      <tr>
        <td><%= tipo_puclet.nome %></td>
        <td><%= link_to 'Show', tipo_puclet %></td>
        <td><%= link_to 'Edit', edit_tipo_puclet_path(tipo_puclet) %></td>
        <td><%= link_to 'Destroy', tipo_puclet, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo Puclet', new_tipo_puclet_path %>
