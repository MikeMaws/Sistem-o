<p id="notice"><%= notice %></p>

<h1>Tipo Habilidades</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_habilidades.each do |tipo_habilidade| %>
      <tr>
        <td><%= tipo_habilidade.nome %></td>
        <td><%= link_to 'Show', tipo_habilidade %></td>
        <td><%= link_to 'Edit', edit_tipo_habilidade_path(tipo_habilidade) %></td>
        <td><%= link_to 'Destroy', tipo_habilidade, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo Habilidade', new_tipo_habilidade_path %>
