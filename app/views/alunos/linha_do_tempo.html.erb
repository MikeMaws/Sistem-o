<body>
 <div id="wrapper">
  <%= render partial: "partial_index" %>
     <div class="container">
      <div class="col-md-12">
       <div class="panel panel-default">
        <div class="panel-body">
         <h1 class="page-header">
          Linha do tempo <small>Computação Paralela</small>
         </h1>
          <div class="list-group">
           <% @tasks.each do |task| %>
            <a href="resolver_task" class="list-group-item">
             <span class="badge pull-right"><%= task.created_at %> </span>
              <i class="fa fa-tasks fa-2x"> <%= task.nome %></i>
            </a>
           <% end %>
          </div>
        </div>
       </div>
      </div>
     </div>
     <!-- /.container-fluid -->
    </div>
  <!-- /#page-wrapper -->
  </div>
<!-- /#wrapper -->
</body>