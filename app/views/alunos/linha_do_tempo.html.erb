<body>
 <div id="wrapper">
  <%= render partial: "partial_index" %>
    <div id="page-wrapper">
     <div class="container-fluid">
      <div class="col-md-12">
       <div class="panel panel-default">
         <div class="panel-body">
          <div class="row">
          <div class="col-md-12">
           <ol class="breadcrumb">
            <li class="active">
             <i class="fa fa-trophy fa-2x"></i><strong> Posição Atual Media : Novato</strong>
            </li>
           </ol>
          </div>
         </div>
         <h1 class="page-header">
          Linha do tempo <small>Computação Paralela</small>
         </h1>
          <div class="list-group">
           <% @tasks.each do |task| %>
           <%= link_to "resolver_task?task_id=#{task.id}", class: "list-group-item" do %>
             <span class="badge pull-right"><%= task.created_at %> </span>
              <i class="fa fa-tasks fa-2x"> <%= task.nome %></i>
              <%end%>
           <% end %>
          </div>
        </div>
       </div>
      </div>
     </div>
     <!-- /.container-fluid -->
    </div>
   </div>
  <!-- /#page-wrapper -->
  </div>
<!-- /#wrapper -->
</body>
